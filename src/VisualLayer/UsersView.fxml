<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="438.0" prefWidth="442.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="VisualLayer.UsersViewController">
    <stylesheets>
        <URL value="@MyStylesheet.css" />
    </stylesheets>
    <children>
        <TableView fx:id="table" layoutX="24.0" layoutY="19.0" prefHeight="354.0" prefWidth="394.0">
            <columns>
                <TableColumn fx:id="userCol" prefWidth="75.0" text="Usuario" />
                <TableColumn fx:id="nameCol" prefWidth="75.0" text="Nombre" />
                <TableColumn fx:id="lnameCol" prefWidth="75.0" text="Apellidos" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor>
        </TableView>
        <Button layoutX="57.0" layoutY="392.0" mnemonicParsing="false" onAction="#onAddPressed" prefHeight="32.0" prefWidth="78.0" text="Agregar" />
        <Button layoutX="309.0" layoutY="392.0" mnemonicParsing="false" onAction="#onElimPressed" text="Eliminar" />
        <Button layoutX="179.0" layoutY="392.0" mnemonicParsing="false" onAction="#modifyUser" text="Modificar" />
    </children>
</AnchorPane>
