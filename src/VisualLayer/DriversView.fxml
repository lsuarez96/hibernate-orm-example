                <TableColumn fx:id="addressCol" prefWidth="175.0" text="Direccion" />
            </columns>
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor>
        </TableView>
        <Button fx:id="add" layoutX="164.0" layoutY="513.0" mnemonicParsing="false" onAction="#onAddPressed" prefHeight="31.0" prefWidth="92.0" text="Agregar" />
        <Button fx:id="mod" layoutX="315.0" layoutY="513.0" mnemonicParsing="false" onAction="#onModifyPressed" text="Modificar" />
        <Button fx:id="del" layoutX="480.0" layoutY="513.0" mnemonicParsing="false" onAction="#onElimPressed" prefHeight="31.0" prefWidth="92.0" text="Eliminar" />
