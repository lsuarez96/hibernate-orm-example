<AnchorPane id="AnchorPane" prefHeight="563.0" prefWidth="703.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="VisualLayer.DriversViewController">
    <stylesheets>
        <URL value="@MyStylesheet.css" />
    </stylesheets>
    <children>
        <TableView fx:id="table" layoutX="42.0" layoutY="51.0" prefHeight="428.0" prefWidth="620.0">
            <columns>
                <TableColumn fx:id="idCol" prefWidth="112.0" text="No. Identidad" />
                <TableColumn fx:id="nameCol" prefWidth="111.0" text="Nombre" />
                <TableColumn fx:id="lastNameCol" prefWidth="132.0" text="Apellidos" />
