<AnchorPane id="AnchorPane" prefHeight="487.0" prefWidth="353.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="VisualLayer.AddorModifyTuristController">
    <stylesheets>
        <URL value="@MyStylesheet.css" />
    </stylesheets>
    <children>
        <Text fx:id="turlb" fill="#c9c8c8" layoutX="105.0" layoutY="52.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Turista">
            <font>
                <Font name="Britannic Bold" size="46.0" />
            </font>
        </Text>
        <Label fx:id="passlb" layoutX="13.0" layoutY="97.0" text="Pasaporte:">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Label>
        <Label fx:id="namelb" layoutX="13.0" layoutY="131.0" text="Nombre:">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Label>
        <Label fx:id="lnamelb" layoutX="13.0" layoutY="173.0" text="Apellidos:">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Label>
        <Label fx:id="phonelb" layoutX="11.0" layoutY="217.0" text="Teléfono:">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Label>
        <Label fx:id="agelb" layoutX="14.0" layoutY="254.0" text="Edad:">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Label>
