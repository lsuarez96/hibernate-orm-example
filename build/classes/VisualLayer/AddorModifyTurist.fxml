        <TextField fx:id="ageTF" layoutX="126.0" layoutY="250.0" onKeyTyped="#keyPressedForDigits" prefHeight="29.0" prefWidth="48.0" />
        <ComboBox fx:id="sexCB" layoutX="117.0" layoutY="292.0" prefHeight="31.0" prefWidth="193.0" promptText="sex" />
        <ComboBox fx:id="countryCB" layoutX="117.0" layoutY="338.0" prefHeight="31.0" prefWidth="215.0" promptText="country" />
    </children>
