        </Label>
        <Label fx:id="countrylb" layoutX="15.0" layoutY="342.0" text="País:">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Label>
        <Button fx:id="acc" layoutX="137.0" layoutY="442.0" mnemonicParsing="false" onAction="#onAcceptPressed" text="Aceptar" />
        <TextField fx:id="idTF" layoutX="129.0" layoutY="91.0" prefHeight="29.0" prefWidth="201.0" promptText="passport  (alfanumeric)" />
        <TextField fx:id="nameTF" layoutX="129.0" layoutY="127.0" onKeyTyped="#keyPressedForLetters" prefHeight="29.0" prefWidth="201.0" promptText="name(only letters)" />
        <TextField fx:id="lastNameTF" layoutX="129.0" layoutY="167.0" onKeyTyped="#keyPressedForLetters" prefHeight="29.0" prefWidth="201.0" promptText="last name (only letters)" />
