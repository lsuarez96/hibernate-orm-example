<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="415.0" prefWidth="365.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="VisualLayer.AddorModifyDriverController">
    <stylesheets>
        <URL value="@MyStylesheet.css" />
    </stylesheets>
    <children>
        <Text fx:id="drivtx" fill="#c9c8c8" layoutX="124.0" layoutY="47.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Chofer">
            <font>
                <Font name="Britannic Bold" size="40.0" />
            </font>
        </Text>
        <Label fx:id="iddrivlb" layoutX="20.0" layoutY="96.0" text="No. ID:" />
        <Label fx:id="namelb" layoutX="20.0" layoutY="142.0" text="Nombre:" />
        <Label fx:id="lnamelb" layoutX="20.0" layoutY="191.0" text="Apellidos:" />
        <Label fx:id="addrelb" layoutX="20.0" layoutY="241.0" text="Dirección:" />
        <Label fx:id="catlb" layoutX="20.0" layoutY="292.0" text="Categoría:" />
        <TextField fx:id="idTF" layoutX="124.0" layoutY="93.0" onKeyTyped="#keyPressedForDigits" prefHeight="29.0" prefWidth="187.0" promptText="No.Id(only 11 digits)" />
        <TextField fx:id="nameTF" layoutX="124.0" layoutY="139.0" onKeyTyped="#keyPressedForLetters" prefHeight="29.0" prefWidth="187.0" promptText="driver's name(only letters)" />
        <TextField fx:id="lastNameTF" layoutX="124.0" layoutY="188.0" onKeyTyped="#keyPressedForLetters" prefHeight="29.0" prefWidth="187.0" promptText="driver's lastname(only letters)" />
        <TextField fx:id="addressTF" layoutX="124.0" layoutY="238.0" prefHeight="29.0" prefWidth="187.0" promptText="driver's address" />
        <ComboBox fx:id="categoryCB" layoutX="124.0" layoutY="291.0" prefHeight="31.0" prefWidth="187.0" promptText="driver's category" />
        <Button fx:id="acc" layoutX="137.0" layoutY="370.0" mnemonicParsing="false" onAction="#onAcceptPressed" prefHeight="31.0" prefWidth="91.0" text="Aceptar" />
    </children>
</AnchorPane>
